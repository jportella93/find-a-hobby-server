{
  "name": "find-a-hobby",
  "version": "1.0.0",
  "description": "Looking for a hobby? The find a hobby app will recommend the best hobbies for you!",
  "main": "server/index.js",
  "keywords": [
    "hobby",
    "recommendation",
    "open",
    "crowdsourced"
  ],
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "jest",
    "lint": "eslint server test",
    "seed": "node server/scripts/seed.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jportella93/find-a-hobby.git"
  },
  "author": "Jon Portella",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jportella93/find-a-hobby/issues"
  },
  "homepage": "https://github.com/jportella93/find-a-hobby#readme",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "mongoose": "^8.6.1",
    "raccoon": "^0.2.8",
    "redis": "^4.6.15",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^23.1.0",
    "nodemon": "^3.1.6",
    "supertest": "^7.0.0"
  }
}
